<div class="devise_login_wrapper">
  <div class="devise_login_form">
    <div class="devise_login_title">Log in</div>

    <%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <%= f.email_field :email, autofocus: true, placeholder: "Required"%>
      <%= f.password_field :password, autocomplete: "off", placeholder: "Required"%>
      <%= f.check_box :remember_me %>
      <br>
      <%= f.submit "Log In", :class => "btn btn-default" %>
    <% end %>

    <div><%= render "devise/shared/links" %></div>

  </div>
</div>
