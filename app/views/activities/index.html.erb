<div class="feed_list">
  <% @activities.each do |activity| %>
    <div class="activity">
      <%= link_to activity.owner.user_name, activity.owner if activity.owner %>
      <%= render_activity activity %>
    </div>
  <% end %>
</div>
<h1> Browse Other Users</h1>
<div class="user_list">
    <% @users.each do |user| %>
    <div>
      <%= image_tag user.avatar, width:"50"%>
      <%= user.user_name %>
      <p> Recipes Created:</p> <%= user.recipes.length %>
      <br>
    </div>
    <% end %>
</div>
