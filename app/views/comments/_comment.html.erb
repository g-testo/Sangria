<li>
  <%= comment.body %> -
  <small>Submitted <%= time_ago_in_words(comment.created_at) %> ago</small>
  <ul>
      <%# if comment.user == current_user %>
      <%= comment.inspect %>
        <li>
          <%= link_to 'Delete Comment',
           recipe_comment_url(@recipe, comment),
           method: :delete,
           data: { confirm: 'Are you sure?' } %>
        </li>
      <%# end %>
  </ul>

</li>
